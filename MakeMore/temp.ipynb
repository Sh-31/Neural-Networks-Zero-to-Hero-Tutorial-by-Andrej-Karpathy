    "import numpy as np\n",
    "\n",
    "def softmax(x):\n",
    "    x = x - x.max()\n",
    "    return np.exp(x) / np.sum(np.exp(x))\n",
    "\n",
    "prob = softmax(np.array([0,0,5,0]))\n",
    "prob"
   ]
